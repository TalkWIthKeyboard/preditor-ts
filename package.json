    "@types/ioredis": "^3.2.7",
    "@types/lodash": "^4.14.106",
    "@types/mongoose": "^5.0.7",
    "@types/mysql": "^2.15.4",
    "@types/node": "^9.6.1",
    "@types/pinyin": "^2.8.0",
